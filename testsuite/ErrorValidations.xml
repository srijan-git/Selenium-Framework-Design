<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

	<groups>
		<run>
			<include name="ErrorHandling" />
		</run>
	</groups>

	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class name="com.SeleniumFrameworkDesign.tests.SubmitOrderTest" />
		</classes>
	</test> <!--Test --><!--thread-count=5 means it will run 5 method of that particular class parallelly-->
	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="com.SeleniumFrameworkDesign.tests.ErrorValidationTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
